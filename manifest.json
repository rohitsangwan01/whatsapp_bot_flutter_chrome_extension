{
  "manifest_version": 2,
  "name": "Whatsapp Bot Flutter",
  "author": "Rohit Sangwan",
  "short_name": "Whatsapp Bot Flutter",
  "description": "Whatsapp bot flutter chrome extension required to communicate with flutter web app",
  "version": "1.0",
  "browser_action": {
  "default_icon":
    {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
   "default_title": "Whatsapp Bot Flutter",
   "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
   "activeTab",
   "storage",
   "tabs",
   "activeTab",
   "http://*/",
   "https://*/"
   ]
}